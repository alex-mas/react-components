{
  "name": "@axc/react-components",
  "version": "0.2.7",
  "description": "react components created for personal use",
  "main": "index.js",
  "scripts": {
    "build": "npm run preBuild && npm run compile",
    "preBuild": "node tasks/preBuild.js",
    "compile": "node_modules/.bin/tsc",
    "genDocs": "node_modules/.bin/typedoc --out ./docs && npm run build-storybook && node tasks/genDocs.js",
    "test:locally": "npm run compile && jest --config=jest.config.json --coverage",
    "test": "npm run compile && jest --config=jest.config.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "preRelease": "npm run build && npm run test:locally",
    "release": "npm run preRelease && node tasks/release.js",
    "release:patch": "npm run preRelease && node tasks/release.js type=patch",
    "release:minor": "npm run preRelease && node tasks/release.js type=minor",
    "release:major": "npm run preRelease && node tasks/release.js typ=major",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o  docs/storybook"
  },
  "homepage": "https://github.com/alex-mas/react-components",
  "url": "TODO",
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-mas/react-components.git"
  },
  "email": "alex.mas.dev@gmail.com",
